public class Main
{
	public static void main(String[] args) {
		int [] arr={1,2,3,4,5,6,7};
		int length=arr.length;
		int l= Math.floorDiv(length, 2);
		int temp;
	    for(int i=0;i<l;i++){
	        temp=arr[i];
	        arr[i]=arr[length-1-i];
	        arr[length-1-i]=temp;
	        for(int element:arr){
	            System.out.print(element);
	        }
	        
	    }
